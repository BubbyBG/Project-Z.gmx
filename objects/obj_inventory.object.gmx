{
    if (global.inventory_state == 0 and point_distance(obj_player.x,obj_player.y,obj_vendor.x,obj_vendor.y) &gt; 32)
    {
        global.inventory_state = 1;
        global.buffer_health = obj_player.stat_health;
        instance_deactivate_all(1);
        instance_create(mouse_x,mouse_y,obj_mouse);
        scr_inventory_create_ammo();
        scr_inventory_create_items(global.inventory_item,0);
        scr_inventory_create_stats();
       
    }
    else if (global.inventory_state == 0 and point_distance(obj_player.x,obj_player.y,obj_vendor.x,obj_vendor.y) &lt;= 32)
    {
        global.inventory_state = 2;
        global.buffer_health = obj_player.stat_health;
        instance_deactivate_all(1);
        instance_create(mouse_x,mouse_y,obj_mouse);
        scr_inventory_create_ammo();
        scr_inventory_create_items(global.inventory_item,0);
        scr_inventory_create_items(global.inventory_merchant,1);
    }
    else if (global.inventory_state &gt; 0 and global.inventory_carry == 0)
    {
        cursor_sprite = -1
        global.inventory_state = 0;
        with (obj_inventory_item) {instance_destroy();}
        with (obj_inventory_ammo) {instance_destroy();}
        with (obj_inventory_statbar) {instance_destroy();}
        with (obj_mouse) {instance_destroy();}
        instance_activate_all();
        obj_player.stat_health = global.buffer_health;
        global.equip_slot[1] = ds_grid_get(global.inventory_item,0,0);
        global.equip_slot[2] = ds_grid_get(global.inventory_item,1,0);
        global.equip_slot[3] = ds_grid_get(global.inventory_item,2,0);
        global.equip_slot[4] = ds_grid_get(global.inventory_item,3,0);
        global.equip_slot[5] = ds_grid_get(global.inventory_item,4,0);
        
        if instance_exists(obj_player)
        {
            with (obj_player)
            {
                gun_current = global.equip_slot[gun_slot_active];
            }
        } 
    }
    else if (global.inventory_state == 1 and global.inventory_carry != 1)
    {
        audio_play_sound(snd_ui_error,1,0);
    }
}

